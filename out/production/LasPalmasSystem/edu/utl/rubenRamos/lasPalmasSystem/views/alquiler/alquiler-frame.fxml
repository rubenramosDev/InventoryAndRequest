<BorderPane minHeight="400.0" minWidth="600.0" styleClass="border-pane" stylesheets="@../../static/css/articulos-styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.utl.rubenRamos.lasPalmasSystem.controllers.alquiler.AlquilerFrameController">
    <top>
        <AnchorPane prefHeight="57.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <HBox alignment="CENTER" prefHeight="62.0" prefWidth="600.0" style="-fx-background-color: #f4f5f7;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <ImageView fitHeight="42.0" fitWidth="78.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../../static/images/reservation.png" />
                            </image>
                            <HBox.margin>
                                <Insets right="15.0" />
                            </HBox.margin>
                        </ImageView>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Alquiler">
                            <font>
                                <Font name="Roboto Regular" size="25.0" />
                            </font>
                        </Text>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </top>
    <center>
        <JFXTabPane BorderPane.alignment="CENTER">
            <tabs>
                <Tab fx:id="tabGrupo" text="Nuevo alquiler">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
                <Tab fx:id="tabAdministrar" text="Administrar">
                    <content>
                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                    </content>
                </Tab>
            </tabs>
        </JFXTabPane>
    </center>
</BorderPane>
